

<div class="section">
  <div class="columns is-multiline">
    <% @caff_files.each do |caff_hash| %>
      <% caff_file = SwaggerClient::SearchResult.build_from_hash caff_hash %>
      <div class="column is-one-quarter">
        <div class="card">
          <div class="card-image">
            <img src="data:image/jpeg;base64,<%= caff_file.preview %>">
          </div>
          <div class="card-footer">
            <a href='# 'class="card-footer-item">Show</a>
            <%= link_to "Download", download_caff_path(:caff_uuid => caff_file.uuid), :method => :get, class: "card-footer-item" %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>
